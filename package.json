  "scripts": {
    "clean": "rm -rf ./generators",
    "compile": "npm run clean && cp -r src/ generators/ && babel src --out-dir generators --ignore **/templates/**",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "npm run clean && cp -r src/ generators/ && babel src --watch --out-dir generators",
    "prepublish": "npm run compile",
    "testIsparte": "babel-node node_modules/.bin/isparta cover _mocha",
    "test": "mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trailsjs/generator-trails.git"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "path-exists": "^2.1.0",
    "detect-conflict": "^1.0.0",
    "falafel": "^1.2.0",
    "generator-node": "trailsjs/generator-node",
    "print-message": "2.1.0",
    "trails": "latest",
    "update-notifier": "0.6.3",
    "yeoman-generator": "^0.23.2",
    "yosay": "1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "coveralls": "2.11.4",
    "isparta": "4.0.0",
    "mocha": "2.4.5",
    "yeoman-assert": "^2.1.2",
    "yeoman-test": "^1.1.0"
  },
  "publishConfig": {
    "tag": "latest"
  },
  "release": {
    "branch": "master"
  }
}
